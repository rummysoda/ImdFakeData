  "imd_voltage_schema": {
    "name": "imd.Voltage",
    "encoding": "jsonschema",
    "schema": {
      "type": "object",
      "title": "IMD Voltage",
      "description": "Insulation Monitoring Device voltage measurements",
      "properties": {
        "MHS_voltage_V": {
          "type": "number",
          "description": "Main High-Side voltage",
          "minimum": 0,
          "maximum": 5,
          "unit": "V"
        },
        "OKHS_voltage_V": {
          "type": "number",
          "description": "OK High-Side voltage",
          "minimum": 0,
          "maximum": 5,
          "unit": "V"
        }
      },
      "required": ["MHS_voltage_V", "OKHS_voltage_V"]
    },
    "topic": "/vehicle/imd/voltage",
    "update_rate_hz": 1000
  },
